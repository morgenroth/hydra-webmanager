<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
 	xmlns:b2="urn:import:com.github.gwtbootstrap.datepicker.client.ui">
	<g:HTMLPanel>
		<b:Hero>
		    <b:Heading size="1">Hydra</b:Heading>
		    <b:Paragraph>The emulation framework for large-scale software testing in disrupted networks.</b:Paragraph>
		</b:Hero>
 		<b:Row>
	    	<b:Column size="12">
				<b:CellTable ui:field="sessionTable" />
	    	</b:Column>
    	</b:Row>
    	<b:Row>
	    	<b:Column size="12">
				<b:Button type="PRIMARY" ui:field="buttonAdd">Add session</b:Button>
	    	</b:Column>
    	</b:Row>
	</g:HTMLPanel>
</ui:UiBinder> 
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	xmlns:b2="urn:import:com.github.gwtbootstrap.datepicker.client.ui">
	<ui:style>
		.pager { margin-top: 12px; }
	</ui:style>
	<b:FluidContainer>
		<b:FluidRow>
			<b:Heading size="2">Slaves</b:Heading>
			<b:CellTable ui:field="slaveTable" />
			<b:SimplePager display="{slaveTable}" location="RIGHT"
				fastForwardRows="15" styleName="{style.pager}" />
		</b:FluidRow>
	</b:FluidContainer>
</ui:UiBinder> 